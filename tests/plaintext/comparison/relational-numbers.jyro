Data.lteFalse = 5 <= 3

# Test 3: Greater than
Data.gt = 5 > 3
Data.gtFalse = 3 > 5
Data.gtEqual = 3 > 3

# Test 4: Greater than or equal
Data.gte = 5 >= 3
