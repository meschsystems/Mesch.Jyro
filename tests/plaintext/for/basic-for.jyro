# Test basic for loop

# Test 1: Count to 5
var result = []
for i in 0 to 5 do
    result = Append(result, i)
end
Data.countUp = result

# Test 2: Sum 1 to 10
var sum = 0
for i in 1 to 11 do
    sum += i
end
Data.sum = sum

# Test 3: Build squares
var squares = []
for i in 1 to 6 do
    squares = Append(squares, i * i)
end
Data.squares = squares
