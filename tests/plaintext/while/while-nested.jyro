# Test nested while loops

# Test 1: Multiplication table row
var result = []
var i = 1
while i <= 3 do
    var row = []
    var j = 1
    while j <= 3 do
        row = Append(row, i * j)
        j += 1
    end
    result = Append(result, row)
    i += 1
end
Data.table = result

# Test 2: Find pair that sums to target
var nums = [2, 7, 11, 15]
var target = 9
var a = 0
var pair = []
var foundPair = false
while a < Length(nums) and not foundPair do
    var b = a + 1
    while b < Length(nums) do
        if nums[a] + nums[b] == target then
            pair = [nums[a], nums[b]]
            foundPair = true
            break
        end
        b += 1
    end
    a += 1
end
Data.pair = pair
