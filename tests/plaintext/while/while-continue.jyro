# Test continue in while loop

# Test 1: Skip even numbers
var i = 0
var odds = []
while i < 10 do
    i += 1
    if i % 2 == 0 then
        continue
    end
    odds = Append(odds, i)
end
Data.odds = odds

# Test 2: Count non-null values
var items = [1, null, 3, null, 5]
var idx = 0
var count = 0
while idx < Length(items) do
    var item = items[idx]
    idx += 1
    if item is null then
        continue
    end
    count += 1
end
Data.nonNull = count

# Test 3: Guard clause pattern
var total = 0
var j = 0
while j < 20 do
    j += 1
    if j % 3 != 0 then
        continue
    end
    total += j
end
Data.sumDiv3 = total
